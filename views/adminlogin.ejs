        crossorigin="anonymous"></script>
       
        <!-- Bootstrap JavaScript -->
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

        <script src="javascripts/register.js"></script>
    
    
</html>